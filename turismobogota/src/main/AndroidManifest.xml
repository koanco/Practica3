<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="johannpolania.com.turismobogota">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".hotels"
            android:label="@string/title_activity_hotels"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".bares"
            android:label="@string/title_activity_bares"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".sitios" />
        <activity android:name=".infoDemografica" />
        <activity android:name=".Autor"></activity>
    </application>

</manifest>
